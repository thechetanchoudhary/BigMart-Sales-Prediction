   "source": [
    "#Linear Regression\n",
    "model = LinearRegression()  \n",
    "outcome_var = 'Item_Outlet_Sales'\n",
    "predictor_var = var\n",
    "regression_model(model, new_train, predictor_var, outcome_var)"
   ]
